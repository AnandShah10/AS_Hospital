<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_hospital_patient" model="ir.sequence">
        <field name="name">Patient Sequence</field>
        <field name="code">hospital.patient.sequence</field>
        <field name="prefix">P</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hospital_case" model="ir.sequence">
        <field name="name">Case Sequence</field>
        <field name="code">hospital.case.sequence</field>
        <field name="prefix">CH</field>
        <field name="padding">7</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_hospital_appointment" model="ir.sequence">
        <field name="name">Appointment Sequence</field>
        <field name="code">hospital.appointment.sequence</field>
        <field name="prefix">AP</field>
        <field name="padding">9</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hospital_doctor" model="ir.sequence">
        <field name="name">Doctor Sequence</field>
        <field name="code">hospital.doctor.sequence</field>
        <field name="prefix">DOC</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hospital_nurse" model="ir.sequence">
        <field name="name">Nurse Sequence</field>
        <field name="code">hospital.nurse.sequence</field>
        <field name="prefix">NUR</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hospital_staff" model="ir.sequence">
        <field name="name">Staff Sequence</field>
        <field name="code">hospital.staff.sequence</field>
        <field name="prefix">SH</field>
        <field name="padding">9</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hospital_bill" model="ir.sequence">
        <field name="name">Bill Sequence</field>
        <field name="code">hospital.bill.sequence</field>
        <field name="prefix">REC</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
